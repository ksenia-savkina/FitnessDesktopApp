<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.*?>
<GridPane xmlns="http://javafx.com/javafx"
          xmlns:fx="http://javafx.com/fxml"
          fx:controller="Controllers.ClassClientsController"
          prefHeight="450.0" prefWidth="400.0">

    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints vgrow="SOMETIMES" percentHeight="10.0"/>
        <RowConstraints vgrow="SOMETIMES"/>
    </rowConstraints>

    <Label stylesheets="@styles.css" text="Клиенты" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
    <TableView fx:id="tableView" prefHeight="200.0" prefWidth="200.0" stylesheets="@styles.css" GridPane.columnIndex="0"
               GridPane.rowIndex="1">
        <columns>
            <TableColumn fx:id="columnFIO" text="ФИО"/>
            <TableColumn fx:id="columnPhone" text="Номер телефона"/>
        </columns>
    </TableView>

</GridPane>
